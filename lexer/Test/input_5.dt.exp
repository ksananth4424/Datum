 FUNC_LABEL
 FUNCTION  ( STRING  IDENTIFIER )  FLOW  IDENTIFIER ( )  FLOW  ( DATASET )  {
     DATASET  IDENTIFIER  =  READ ( IDENTIFIER ,  STRING_LITERAL ) ;
     RETURN  IDENTIFIER ;
 }

 FUNCTION  ( ARRAY  IDENTIFIER ,  INTEGER  IDENTIFIER )   FLOW  IDENTIFIER ( )  FLOW  ( FLOAT )  {
     FLOAT  SUM  =  CONSTANT ;
     LOOP  IDENTIFIER  FROM  CONSTANT  TO  IDENTIFIER  {
         SUM  ADD_ASSIGN  IDENTIFIER [ IDENTIFIER ] ;
     }
     RETURN  SUM  /  IDENTIFIER ( IDENTIFIER ) ;
 }

 START_LABEL
 INTEGER  IDENTIFIER  =  CONSTANT ;
 FLOAT  IDENTIFIER  =  CONSTANT ;
 STRING  IDENTIFIER  =  STRING_LITERAL ;
 BOOL  IDENTIFIER  =  IDENTIFIER ;
 ARRAY  IDENTIFIER  =  [ CONSTANT ,  CONSTANT ,  CONSTANT ,  CONSTANT ,  CONSTANT ] ;
 DATASET  IDENTIFIER ;

 STRING  IDENTIFIER  =  STRING_LITERAL
 IDENTIFIER  =  IDENTIFIER  FLOW  IDENTIFIER ( ) ;
 IDENTIFIER  =  ( IDENTIFIER , CONSTANT )  FLOW  IDENTIFIER ( ) ;

 IF  ( IDENTIFIER  AND  IDENTIFIER  >  CONSTANT )  {
     INTEGER  IDENTIFIER  =  IDENTIFIER  FLOW  MAX ( ) ;
     INTEGER  IDENTIFIER  =  IDENTIFIER  FLOW  MIN ( ) ;
     STRING_LITERAL  FLOW  SHOW ( ) ;
     IDENTIFIER  FLOW  SHOW ( ) ;
     STRING_LITERAL  FLOW  SHOW ( ) ;
     IDENTIFIER  FLOW  SHOW ( ) ;
 }  ELSE  {
     STRING_LITERAL  FLOW  SHOW ( ) ;
 }

 LOOP  IDENTIFIER  FROM  CONSTANT  TO  CONSTANT  STEP  CONSTANT  {
     INTEGER  IDENTIFIER  =  CONSTANT ;
     STRING_LITERAL  FLOW  SHOW ( ) ;
     IDENTIFIER [ IDENTIFIER ]  FLOW  SHOW ( ) ;
     IF  ( IDENTIFIER [ IDENTIFIER ]  %  CONSTANT  EQ_OP  CONSTANT )  {
         IDENTIFIER  ADD_ASSIGN  IDENTIFIER [ IDENTIFIER ] ;
     }  ELSE  {
         STRING_LITERAL  FLOW  SHOW ( ) ;
         IDENTIFIER [ IDENTIFIER ]  FLOW  SHOW ( ) ;
     }
 }